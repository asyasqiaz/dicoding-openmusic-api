{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": 0,
    "newline-per-chained-call": "off",
    "camelcase": "off",
    "no-unused-vars": "off",
    "function-paren-newline": "off",
    "radix": "off"
  }
}
